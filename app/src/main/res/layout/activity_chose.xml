<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.ChoseActivity">

    <Button
        android:id="@+id/btn_chose_toblock"
        android:layout_width="191dp"
        android:layout_height="70dp"
        android:text="区块链溯源"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.41000003" />

    <Button
        android:id="@+id/btn_chose_tonone"
        android:layout_width="191dp"
        android:layout_height="70dp"
        android:text="非区块链溯源"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_chose_toblock"
        app:layout_constraintStart_toStartOf="@+id/btn_chose_toblock"
        app:layout_constraintTop_toBottomOf="@+id/btn_chose_toblock"
        app:layout_constraintVertical_bias="0.18" />
</androidx.constraintlayout.widget.ConstraintLayout>